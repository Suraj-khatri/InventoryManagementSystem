@model  IEnumerable<InventoryManagementSystem.Domain.FixedAssetsViewModel.changesApprovalQueueVM>
    @{
    ViewBag.Title = "Asset Requisition";
    }
    <div class="box box-primary">
        <div class="box-header with-border">
            <div class="box-title"><i class="fa fa-list"></i> Asset Requisition Details </div>
            <div class="box-tools pull-right">
                <button class="btn btn-box-tool" type="button" data-widget="collapse">
                    <i class="fa fa-minus"></i>
                </button>
            </div>
        </div>
        <div class="box-body tablefont">
            <table class="table table-bordered">
                <thead class="thead-dark">
                    <tr>
                        <th>Table ID</th>
                        <th>ID Field</th>
                        <th>Request Nature</th>
                        <th>Table Name</th>
                        <th>Table Desc</th>
                        <th>Request By	</th>
                        <th>Request Date</th>
                        <th>View</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                    <tr>
                        <td>
                            @Html.DisplayFor(model => item.dataid)
                        </td>
                        <td>
                            @Html.DisplayFor(model => item.identifierField)
                        </td>
                        <td>
                            @Html.DisplayFor(model => item.modType)
                        </td>
                        <td>
                            @Html.DisplayFor(model => item.tableName)
                        </td>
                        <td>
                            @Html.DisplayFor(model => item.description)
                        </td>
                        <td>
                            @Html.DisplayFor(model => item.createdBy)
                        </td>
                        <td>
                            @Html.DisplayFor(model => item.createdDate)
                        </td>
                        <td>
                            <a href="@Url.Action("ViewAssetBooking","ApprovalNotification",new{Id=item.dataid })" class="btn btn-xs btn-info" title="View"><i class="fa fa-eye"></i></a>
                        </td>
                    </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>

