@model InventoryManagementSystem.Domain.ReportingViewModel.ReportParameterVM
@using ReportViewerForMvc;

@{
    ViewBag.Title = "Purchase Invoice";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@if (ViewBag.ReportViewer != null)
{
    @Html.ReportViewer(ViewBag.ReportViewer as Microsoft.Reporting.WebForms.ReportViewer, new { scrolling = "no" });
}
