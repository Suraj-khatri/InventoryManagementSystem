@model  IEnumerable<InventoryManagementSystem.Domain.InterBranchTransferACKVM>@{    ViewBag.Title = "Inter Branch Transfer Details";}<div class="box box-primary">    <div class="box-header with-border">        <div class="box-title"> <i class="fa fa-arrow-right"></i>Inter Branch Transfer Details</div>        <div class="box-tools pull-right">            <button class="btn btn-box-tool" type="button" data-widget="collapse">                <i class="fa fa-minus"></i>            </button>        </div>    </div>    @using (Html.BeginForm())    {@Html.AntiForgeryToken()// @Html.HiddenFor(x => x.n)<div class="box-body tablefont">    @Html.ValidationSummary(true, "", new { @class = "text-danger" })    <h4 style="margin-bottom:10px; color:chocolate"><i class="fa fa-info-circle"></i> Inter Branch Transfer Details</h4>    <div id="tadd">        <table class="table table-bordered" id="tbl_create">            <thead class="thead-dark" style="height:20px">                <tr>                    <th>Product Code</th>                    <th>Product Name</th>                    <th>Quantity</th>                    <th>Unit</th>                    <th class="hidden">Rate</th>                </tr>            </thead>            <tbody>                @foreach (var item in Model)                {<tr>    <td class="hidden">        @Html.DisplayFor(model => item.Inter_Branch_TransferId)    </td>    <td>        @Html.DisplayFor(model => item.ProductId)    </td>    <td>        @Html.DisplayFor(model => item.ProductName)    </td>    <td>        @Html.DisplayFor(model => item.Qty)    </td>    <td>        @Html.DisplayFor(model => item.Unit)    </td>    <td class="hidden">        @Html.DisplayFor(model => item.Rate)    </td></tr>}            </tbody>        </table>    </div>    <fieldset style="list-style: circle; list-style-type: circle; width: 98%;">        <legend style="color:forestgreen;font:bold;font-size:small">InterBranch Information</legend>        <input type="hidden" name="Tid" id="hiddenTid" value="@ViewBag.Tid" class="form-control">        <div class="row">            <div class="col-md-12 col-xs-12">                <div class="form-group">                    <label>Transferred Message</label>                    @Html.TextArea("Narration", ViewBag.Narration as string, new { rows = "3", cols = "150", @disabled = "disabled", @class = "form-control" })                </div>            </div>        </div>        <div class="row">            <div class="col-md-12 col-xs-12">                <div class="form-group">                    <label>Acknowledge Remarks</label>                    @Html.TextArea("Acknowledge Remarks", new { rows = "3", cols = "150", id = "AckRemark", @class = "form-control" })                </div>            </div>        </div>    </fieldset>    <div class="box-footer">        <div class="col-md-12 col-md-offset-8">            <button type="button" id="saveacknowledge" class="btn btn-success"><i class="fa fa-save"></i> Acknowledge</button>            <a href="/InterBranchTransfer/Index" class="btn btn-default"><i class="fa fa-close"></i> Cancel</a>        </div>    </div></div>}</div><script src="~/Scripts/Areas/InterBranchTransfer/ViewDetails.js"></script>