@model InventoryManagementSystem.Domain.InterBranchTransferVM@{    ViewBag.Title = "InterBranch Transfer";}<style>    input {        height: 3rem;    }</style><div class="box box-primary">    <div class="box-header with-border">        <div class="box-title"> <i class="fa fa-arrow-right"></i>Inter-Branch Transfer</div>        <div class="box-tools pull-right">            <button class="btn btn-box-tool" type="button" data-widget="collapse">                <i class="fa fa-minus"></i>            </button>        </div>    </div>    @using (Html.BeginForm())    {@Html.AntiForgeryToken()<div class="box-body formfont">    @Html.ValidationSummary(true, "", new { @class = "text-danger" })    <p>Please enter valid data! <a style="color:red">(* Required fields)</a></p>    <h4 style="margin-bottom:10px; color:chocolate"><i class="fa fa-info-circle"></i> Place Transfer Details</h4>    <div class="row">        <div class="col-md-4 col-md-12">            <div class="form-group">                @Html.LabelFor(model => model.ToBranchId, htmlAttributes: new { @class = "control-label" })                @Html.DropDownListFor(model => model.ToBranchId, Model.BranchList, new { @class = "form-control selectSearch", id = "ToBranchName" })                @Html.ValidationMessageFor(model => model.ToBranchId, "", new { @class = "text-danger" })            </div>        </div>        <div class="col-md-4 col-md-12">            <div class="form-group">                @Html.LabelFor(model => model.ToDeptId, htmlAttributes: new { @class = "control-label" })                @Html.DropDownListFor(model => model.ToDeptId, Model.DepartmentList, new { @class = "form-control selectSearch", id = "ToDeptName" })                @Html.ValidationMessageFor(model => model.ToDeptId, "", new { @class = "text-danger" })            </div>        </div>        <div class="col-md-4 col-md-12">            <div class="form-group">                @Html.LabelFor(model => model.SenderTo, htmlAttributes: new { @class = "control-label" })                @Html.DropDownListFor(model => model.SenderTo, Model.EmployeeList, new { @class = "form-control selectSearch", id = "SendTo" })                @Html.ValidationMessageFor(model => model.SenderTo, "", new { @class = "text-danger" })            </div>        </div>    </div>    <fieldset style="list-style: circle; list-style-type: circle; width: 98%;">        <legend style="color:forestgreen;font-size:small;font:bold">Product Information:</legend>        <div class="row">            <div class="col-md-4 col-md-12">                <div class="form-group">                    @Html.LabelFor(model => model.ProdGroupId, htmlAttributes: new { @class = "control-label" })                    @Html.DropDownListFor(model => model.ProdGroupId, Model.ProductGroupList, new { @class = "form-control", id = "prodgroup" })                    @Html.ValidationMessageFor(model => model.ProdGroupId, "", new { @class = "text-danger" })                </div>            </div>            <div class="col-md-4 col-md-12">                <div class="form-group">                    @Html.LabelFor(model => model.ProductId, htmlAttributes: new { @class = "control-label" })                    @Html.DropDownListFor(model => model.ProductId, Model.ProductList, new { @class = "form-control selectSearch", id = "ProductName" })                    @Html.ValidationMessageFor(model => model.ProductId, "", new { @class = "text-danger" })                </div>            </div>            <div class="col-md-1 col-xs-12">                <div class="form-group">                    <label class="control-label" style="color:#fff"> Add</label>                    <button type="button" class="btn btn-xs btn-success" id="adddata" title="Add">Add</button>                </div>            </div>        </div>        <div id="tadd">            <table class="table table-bordered noDataTable" id="tbl_create">                <thead class="thead-dark" style="height:20px">                    <tr>                        <th>Product Id</th>                        @*<th class="hidden">Tem Req Id</th>*@                        <th>Product Name</th>                        <th>Qty</th>                        <th>Rate</th>                        <th>Stock In Branch(From)</th>                        @*<th>Stock In Branch(To)</th>*@                        <th>Unit</th>                        <th>Action</th>                        <th>&nbsp;</th>                    </tr>                </thead>                <tbody id="datalist"></tbody>            </table>        </div>    </fieldset>    <fieldset style="list-style: circle; list-style-type: circle; width: 98%;">        <legend style="color:forestgreen;font:bold;font-size:small">Other Information:</legend>        <div class="row">            <div class="col-md-4 col-md-12">                <div class="form-group">                    <label class="control-label required">Message </label>                    @Html.TextAreaFor(model => model.Narration, new { rows = "3", cols = "150", id = "Narration" })                    @Html.ValidationMessageFor(x => x.Narration, "", new { @class = "text-danger" })                </div>            </div>        </div>        <div class="row">            <div class="col-md-4 col-md-12">                <div class="form-group">                    @Html.LabelFor(model => model.FromBranchId, htmlAttributes: new { @class = "control-label" })                    @Html.DropDownListFor(model => model.FromBranchId, Model.BranchList, new { @class = "form-control selectSearch", id = "FromBranchName" })                    @Html.ValidationMessageFor(model => model.FromBranchId, "", new { @class = "text-danger" })                </div>            </div>            <div class="col-md-4 col-md-12">                <div class="form-group">                    @Html.LabelFor(model => model.FromDeptId, htmlAttributes: new { @class = "control-label" })                    @Html.DropDownListFor(model => model.FromDeptId, Model.DepartmentList, new { @class = "form-control selectSearch", id = "FromDeptName" })                    @Html.ValidationMessageFor(model => model.FromDeptId, "", new { @class = "text-danger" })                </div>            </div>            <div class="col-md-4 col-md-12" style="display: none;">                <div class="form-group">                    @Html.LabelFor(model => model.SenderFrom, htmlAttributes: new { @class = "control-label" })                    @Html.DropDownListFor(model => model.SenderFrom, Model.EmployeeList, new { @class = "form-control selectSearch", id = "SenderFrom" })                    @Html.ValidationMessageFor(model => model.SenderFrom, "", new { @class = "text-danger" })                </div>            </div>        </div>    </fieldset>    <div class="box-footer">        <div class="col-md-12 col-md-offset-9">            <button type="button" id="transfer" class="btn btn-success"><i class="fa fa-save"></i> Save</button>            <a href="/InterBranchTransfer/Create" class="btn btn-default"><i class="fa fa-close"></i> Cancel</a>        </div>    </div></div>}</div><div class="modal fade" id="myModal" role="dialog" tabindex="-1">    <div class="modal-dialog">        <div class="modal-content">            <div class="modal-header">                <button type="button" class="close" data-dismiss="modal">&times;</button>                <h4 class="modal-title" style="color:chocolate"><i class="fa fa-info-circle fa-lg"></i> <strong>Product Other Information Test</strong></h4>            </div>            <div class="modal-body list-modal">                <div id="prodinfo">                </div>            </div>            <div class="modal-footer">                <a class="btn btn-xs btn-success serialnosave" title="Save">Save</a>            </div>        </div>    </div></div><script src="~/Scripts/Areas/InterBranchTransfer/Create.Js"></script>