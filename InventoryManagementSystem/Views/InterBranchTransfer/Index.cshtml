@model  IEnumerable<InventoryManagementSystem.Domain.InterBranchTransferACKVM>@{    ViewBag.Title = "Inter Branch Transfer";}<div class="box box-primary">    <div class="box-header with-border">        <div class="box-title"><i class="fa fa-list"></i> Inter Branch Transfer</div>        <div class="box-tools pull-right">            <button class="btn btn-box-tool" type="button" data-widget="collapse">                <i class="fa fa-minus"></i>            </button>        </div>    </div>    <div class="box-body tablefont" style="overflow:auto">        <table class="table table-bordered noDataTable">            <thead class="thead-dark">                <tr>                    @*<th>Dispatch. Id</th>*@                    <th>TransferType</th>                    <th>From <br /> Branch</th>                    <th>To<br />Branch</th>                    <th>TransferedBy <br /> Dept.</th>                    <th>TransferedTo <br /> By</th>                    <th>From<br />Department </th>                    <th>To <br /> Department</th>                    <th>TransferedDate</th>                    <th>Acknowledge</th>                </tr>            </thead>            <tbody>                @foreach (var item in Model)                {    <tr>        @*<td>                @Html.DisplayFor(model => item.dispatchedid)            </td>*@        <td>            @Html.DisplayFor(model => item.TransferType)        </td>        <td>            @Html.DisplayFor(model => item.FromBranch)        </td>        <td>            @Html.DisplayFor(model => item.ToBranch)        </td>        <td>            @Html.DisplayFor(model => item.TransferedBy)        </td>        <td>            @Html.DisplayFor(model => item.TransferedTo)        </td>        <td>            @Html.DisplayFor(model => item.FromDepartment)        </td>        <td>            @Html.DisplayFor(model => item.ToDepartment)        </td>        <td>            @Html.DisplayFor(model => item.TransferedDate)        </td>        <td>            <a href="@Url.Action("ViewTransferDetails","InterBranchTransfer",new { id=item.Id})" class="btn btn-xs btn-info" title="View"><i>View</i></a>        </td>    </tr>}            </tbody>        </table>    </div></div><script>    $(document).ready(function () {        $('.table').DataTable({            "order": [[5, 'desc']],            "pageLength": 25        });    });</script>